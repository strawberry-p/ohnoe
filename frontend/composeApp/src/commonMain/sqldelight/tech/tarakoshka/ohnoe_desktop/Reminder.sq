CREATE TABLE Reminder (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    text TEXT NOT NULL,
    messages TEXT NOT NULL,
    time INTEGER NOT NULL,
    completed INTEGER NOT NULL DEFAULT 0
);

insertReminder:
INSERT INTO Reminder(text, messages, time)
VALUES (?, ?, ?);

getAllReminders:
SELECT *
FROM Reminder
ORDER BY time;

completeReminder:
UPDATE Reminder
SET completed = 1
WHERE id = (?);

getInRange:
SELECT *
FROM Reminder
WHERE completed = 0
AND time - (?) < (?);